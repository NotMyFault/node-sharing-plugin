<com.redhat.jenkins.nodesharingfrontend.SharedNode>
  <name>solaris1.acme.com</name>
  <remoteFS>./var/jenkins-workspace</remoteFS>
  <numExecutors>1</numExecutors>
  <launcher class="hudson.plugins.sshslaves.SSHLauncher" plugin="ssh-slaves">
    <host></host>
    <port>22</port>
    <credentialsId></credentialsId>
    <javaPath>/path/to/launcher</javaPath>
    <launchTimeoutSeconds>600</launchTimeoutSeconds>
    <maxNumRetries>0</maxNumRetries>
    <retryWaitTime>0</retryWaitTime>
  </launcher>
  <label>solaris solaris11 sparc</label>
  <nodeProperties/>
</com.redhat.jenkins.nodesharingfrontend.SharedNode>
